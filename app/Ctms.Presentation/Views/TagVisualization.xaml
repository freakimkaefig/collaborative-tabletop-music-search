<s:TagVisualization x:Class="Ctms.Presentation.Views.TagVisualization"
                    
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:s="http://schemas.microsoft.com/surface/2008"
    >
    <!--Loaded="SimpleVisualization_Loaded"-->
    <Grid>
        <s:TagVisualizer x:Name="TagVisualizer" Width="Auto" Height="Auto">

            <Canvas Name="ApplicationCanvas" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Height="160" Width="200">
                <Label Content="Tag-Visualisierung!" Height="40" HorizontalAlignment="Left" Margin="0,-40,0,0" Name="VCard_Name" VerticalAlignment="Top" Width="156" FontSize="16" />

                <Rectangle Height="120" Width="200" Name="MainCanvas" Fill="#57FF3ACB" VerticalAlignment="Bottom" HorizontalAlignment="Center">

                </Rectangle>
            </Canvas>
        </s:TagVisualizer>
</Grid>
</s:TagVisualization>